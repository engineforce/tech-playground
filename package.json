{
  "name": "typescript-checkjs",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "link": "lerna link && lerna run link --since --stream",
    "build": "lerna run build --since --stream",
    "clean": "lerna clean --yes && rm -rf node_modules",
    "check": "lerna run check --since --parallel",
    "test": "lerna run test --since --parallel",
    "test-integration": "lerna run test-integration --since --parallel",
    "package": "lerna run package --since --stream",
    "deploy": "lerna run deploy --since --stream",
    "undeploy": "lerna run undeploy --since --stream",
    "precommit": "pretty-quick --staged"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.56",
    "@babel/core": "^7.0.0-beta.56",
    "@babel/plugin-transform-classes": "^7.0.0-beta.56",
    "@babel/preset-env": "^7.0.0-beta.56",
    "@types/aws-sdk": "^2.7.0",
    "@types/chance": "^1.0.1",
    "@types/jest": "^24.0.0",
    "@types/lodash": "^4.14.109",
    "@types/node": "^10.3.1",
    "@types/react-native": "^0.57.34",
    "@types/recompose": "^0.30.3",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.0-beta",
    "babel-plugin-lodash": "^3.3.4",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "gatsby-cli": "^2.4.9",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "lerna": "^3.11.0",
    "poi": "^12.5.5",
    "polymer-cli": "^1.7.7",
    "prettier": "^1.13.5",
    "pretty-quick": "^1.6.0",
    "regenerator-runtime": "^0.13.1",
    "serverless": "^1.28.0",
    "serverless-plugin-tracing": "^2.0.0",
    "serverless-webpack": "^5.2.0",
    "typescript": "^3.0.1",
    "webpack": "^4.14.0"
  },
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*",
      "services/*"
    ],
    "nohoist": [
      "**/portfolio-web",
      "**/portfolio-web/**",
      "**/investing-rn",
      "**/investing-rn/**",
      "**/chart-web",
      "**/chart-web/**"
    ]
  }
}
