{
  "name": "typescript-checkjs",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "link": "lerna link && lerna run link --since --stream",
    "build": "lerna run build --since --stream",
    "clean": "lerna clean --yes && rm -rf node_modules",
    "check": "lerna run check --since --parallel",
    "test": "lerna run test --since --parallel",
    "test-integration": "lerna run test-integration --since --parallel",
    "package": "lerna run package --since --stream",
    "deploy": "lerna run deploy --since --stream",
    "undeploy": "lerna run undeploy --since --stream",
    "precommit": "pretty-quick --staged"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/chance": "^1.0.1",
    "@types/jest": "^23.1.3",
    "@types/lodash": "^4.14.109",
    "@types/node": "^10.3.1",
    "@types/react-native": "^0.55.26",
    "@types/recompose": "^0.26.1",
    "gatsby-cli": "^1.1.58",
    "husky": "^0.14.3",
    "lerna": "^2.11.0",
    "poi": "^10.2.9",
    "polymer-cli": "^1.7.7",
    "prettier": "^1.13.5",
    "pretty-quick": "^1.6.0",
    "serverless": "^1.28.0"
  },
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*",
      "services/*"
    ],
    "nohoist": [
      "**/portfolio-web",
      "**/portfolio-web/**",
      "**/investing-rn",
      "**/investing-rn/**",
      "**/chart-web",
      "**/chart-web/**"
    ]
  }
}